<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.it.salemanegement">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.it.salemanegement.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.it.salemanegement.MenuActivity" />
        <activity android:name="com.it.salemanegement.MemberActivity" />
        <activity android:name="com.it.salemanegement.ProductActivity" />
        <activity android:name="com.it.salemanegement.FinanceActivity" />
        <activity android:name="com.it.salemanegement.InfoActivity" />

        <service
            android:name="com.it.salemanegement.JoinService"
            android:enabled="true"
            android:exported="true"></service>
    </application>

</manifest>